{
  "name": "storeon-sessionstorage",
  "version": "0.6.3",
  "description": "Module for storeon to persist data from state to session storage",
  "repository": "koddr/storeon-sessionstorage",
  "author": "Vic Sh√≥stak <vikkyshostak@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint *.js test/*.js test/demo/*.js --fix",
    "size": "size-limit",
    "spell": "npm run docs && yaspeller *.md",
    "docs": "documentation build *.js -f md -o api.md",
    "test": "jest --coverage && npm run lint && npm run size && npm run spell",
    "start": "parcel serve --cache-dir test/demo/.cache -d test/demo/dist test/demo/index.html --open"
  },
  "devDependencies": {
    "@logux/eslint-config": "^35.0.0",
    "@size-limit/preset-small-lib": "^3.0.1",
    "clean-publish": "^1.1.2",
    "documentation": "^13.0.2",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-es5": "^1.4.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^13.0.0",
    "husky": "^5.0.9",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "parcel-bundler": "^1.12.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "storeon": "^3.0.3",
    "yaspeller": "^7.0.0"
  },
  "size-limit": [
    {
      "path": "index.js",
      "limit": "172 B"
    }
  ],
  "eslintConfig": {
    "extends": "@logux/eslint-config/browser",
    "rules": {
      "consistent-return": "off",
      "func-style": "off"
    }
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.md": "yaspeller"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "dictionary": [
      "storeon",
      "persistState",
      "sessionstorage",
      "redux",
      "gzipped",
      "Versioning",
      "param",
      "ES5",
      "sessionStorage",
      "Tomas",
      "Sandven",
      "eslint",
      "Menshykov",
      "localstorage",
      "localStorage"
    ]
  }
}
